<h3>
    <!--<a href="<%= model.get("url") %>">-->
        <%= model.get("lijstnummer") %>. <%= model.get("naam") %>
    <!--</a>-->
</h3>
<section class="kandidaten">
    <% if (kandidaten) { %>
        <ol>
            <% kandidaten.forEach(function (kandidaat) { %>
                <li>
                    <h4><%= kandidaat.get("position") %>. <%= kandidaat.get("full_name") %></h4>
                    <p><%= kandidaat.get("gender") %>, <%= kandidaat.get("place") %></p>
                </li>
            <% }) %>
        </ol>
    <% } %>
<section>
